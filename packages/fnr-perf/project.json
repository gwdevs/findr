{
  "name": "fnr-perf",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/fnr-perf/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p packages/fnr-mui/tsconfig.node.json",
        "outputPath": "dist/packages/fnr-mui"
      },
      "dependsOn": ["build-browser"]
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs fnr-perf {args.ver} {args.tag} {args.otp}"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/fnr-perf/**/*.js"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/fnr-perf/jest.config.js",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
