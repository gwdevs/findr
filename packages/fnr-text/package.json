{
  "name": "@findr/text",
  "version": "1.0.7",
  "type": "commonjs",
  "files": [
    "./"
  ],
  "bugs": "https://github.com/gwdevs/findr/issues",
  "homepage": "https://github.com/gwdevs/findr/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gwdevs/findr.git"
  },
  "scripts": {
    "docs": "typedoc --options ./src/typedoc.json"
  },
  "keywords": [
    "search",
    "find",
    "replace",
    "text"
  ],
  "peerDependenciesMeta": {
    "xregexp": {
      "optional": true
    }
  },
  "devDependencies": {
    "typedoc": "^0.24.8",
    "typedoc-plugin-rename-defaults": "^0.6.4",
    "typedoc-theme-hierarchy": "^3.0.0"
  },
  "main": "./index.cjs",
  "module": "./index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs"
    },
    "./multiline": {
      "import": "./multiline/index.mjs",
      "require": "./multiline/index.cjs"
    }
  }
}
